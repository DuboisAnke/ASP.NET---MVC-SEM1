@model ProjectCWeb_DuboisAnke_2ProA.Models.Inschrijving

@{
    ViewData["Title"] = "Details";
}
<link rel="stylesheet" href="~/css/personalstyling.css" />
<link rel="stylesheet" href="~/css/footerstyle.css" />

<br />
<h1>DETAILS</h1>

<div>
    <hr />
    <dl class="row body-style">
        <dt class = "col-sm-2">
            <label class="h6">ID</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.InschrijvingId)
        </dd>
        <dt class="col-sm-2">
            <label class="h6">STUDENT</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student.Gebruiker.Naam)
        </dd>
        <dt class="col-sm-2">
            <label class="h6">VAKLECTOR</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VakLector.Lector.Gebruiker.Naam)
        </dd>
        <dt class="col-sm-2">
            <label class="h6">ACADEMIEJAAR</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Academiejaar.StartDatum)
        </dd>
    </dl>
</div>
<hr/>
<div>
    <a asp-action="Edit" asp-route-id="@Model.InschrijvingId">Bewerk</a> |
    <a asp-action="Index">Terug naar INSCHRIJVINGEN</a>
</div>
